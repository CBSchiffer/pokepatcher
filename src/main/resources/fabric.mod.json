{
	"schemaVersion": 1,
	"id": "pokepatcher",
	"version": "${version}",
	"name": "Clokkworkk's PokePatcher",
	"description": "Converts Cobblemon Datapacks to Fabric Mods to be used with Cobblemon. Designed to workaround a bug preventing datapack mons from loading properly.",
	"authors": [
		"Clokkworkk"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/CBSchiffer/pokepatcher"
	},
	"license": "MIT",
	"icon": "assets/pokepatcher/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.clokkworkk.pokepatcher.PokePatcher"
		],
		"preLaunch": [
			"com.clokkworkk.pokepatcher.PreLaunchManager"
		]
	},
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.1",
		"cobblemon": ">=1.6.1",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}